DATA: lt_range TYPE RANGE OF i,
      ls_range TYPE LINE OF lt_range.

ls_range-sign = 'I'.
ls_range-option = 'BT'. " BT = Between
ls_range-low = 10.
ls_range-high = 20.
APPEND ls_range TO lt_range.

ls_range-sign = 'I'.
ls_range-option = 'BT'. " Between
ls_range-low = 30.
ls_range-high = 40.
APPEND ls_range TO lt_range.
